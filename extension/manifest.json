{
    "manifest_version": 3,
    "name": "Rekorder.io",
    "description": "Screen Recorder on steroids | AI powered product demo creator and editor",
    "version": "1.0.0",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {},
    "background": {
        "service_worker": "build/background.js"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "build/*",
                "assets/*",
                "icons/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "storage",
        "offscreen",
        "scripting",
        "activeTab",
        "tabCapture",
        "webNavigation",
        "downloads"
    ],
    "externally_connectable": {
        "matches": [
            "https://*.screech.ai/*",
            "http://192.168.10.157:4200/*"
        ],
        "accept_tls_channel_id": true
    }
}